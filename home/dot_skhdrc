cmd - return : open -a ghostty
shift + cmd - m : open -na emacs
cmd - f1 : pmset displaysleepnow

shift + cmd - c : yabai -m space --create
alt + cmd - c : yabai -m space --destroy

shift + cmd - left : yabai -m window --warp west
shift + cmd - right : yabai -m window --warp east

cmd - left : yabai -m space --focus prev
cmd - right : yabai -m space --focus next

shift + cmd - left : yabai -m window --space prev; yabai -m space --focus prev
shift + cmd - right : yabai -m window --space next; yabai -m space --focus next

#cmd - 0x12 : yabai -m space --focus 1
#cmd - 0x13 : yabai -m space --focus 2
#cmd - 0x14 : yabai -m space --focus 3
#cmd - 0x15 : yabai -m space --focus 4
#cmd - 0x17 : yabai -m space --focus 5
#cmd - 0x16 : yabai -m space --focus 6
#cmd - 0x1A : yabai -m space --focus 7
#cmd - 0x1C : yabai -m space --focus 8
#cmd - 0x19 : yabai -m space --focus 9
#cmd - 0x1D : yabai -m space --focus 10

shift + cmd - 0x12 : yabai -m window --space 1
shift + cmd - 0x13 : yabai -m window --space 2
shift + cmd - 0x14 : yabai -m window --space 3
shift + cmd - 0x15 : yabai -m window --space 4
shift + cmd - 0x17 : yabai -m window --space 5
shift + cmd - 0x16 : yabai -m window --space 6
shift + cmd - 0x1A : yabai -m window --space 7
shift + cmd - 0x1C : yabai -m window --space 8
shift + cmd - 0x19 : yabai -m window --space 9
shift + cmd - 0x1D : yabai -m window --space 10

shift + cmd - space : yabai -m window --toggle float && yabai -m window --grid 4:4:1:1:2:2
shift + cmd - f : yabai -m window --toggle native-fullscreen
shift + cmd - x : yabai -m space --display next || yabai -m space --display prev
shift + cmd - y : yabai -m window --display next || yabai -m window --display prev
shift + cmd - e : yabai -m window --toggle split

shift + cmd - r : brew services restart yabai

cmd - f7 : ~/.scripts/theme_switch.sh
