{
  "document": {
    "color": "#e0def4",
    "margin": 2
  },
  "block_quote": {
    "color": "#f6c177",
    "italic": true,
    "indent": 2
  },
  "list": {
    "color": "#e0def4",
    "level_indent": 2
  },
  "heading": {
    "color": "#c4a7e7",
    "bold": true
  },
  "h1": {
    "prefix": "# "
  },
  "h2": {
    "prefix": "## "
  },
  "h3": {
    "prefix": "### "
  },
  "h4": {
    "prefix": "#### "
  },
  "h5": {
    "prefix": "##### "
  },
  "h6": {
    "prefix": "###### "
  },
  "strikethrough": {
    "crossed_out": true
  },
  "emph": {
    "color": "#f6c177",
    "italic": true
  },
  "strong": {
    "color": "#f6c177",
    "bold": true
  },
  "hr": {
    "color": "#524f67",
    "format": "\n--------\n"
  },
  "item": {
    "block_prefix": "• "
  },
  "enumeration": {
    "color": "#9ccfd8"
  },
  "task": {
    "ticked": "[✓] ",
    "unticked": "[ ] "
  },
  "link": {
    "color": "#c4a7e7",
    "underline": true
  },
  "link_text": {
    "color": "#c4a7e7"
  },
  "image": {
    "color": "#c4a7e7",
    "underline": true
  },
  "image_text": {
    "color": "#c4a7e7",
    "format": "Image: {{.text}} →"
  },
  "code": {
    "color": "#31748f"
  },
  "code_block": {
    "margin": 2,
    "chroma": {
      "text": {
        "color": "#e0def4"
      },
      "error": {
        "color": "#e0def4",
        "background_color": "#c4a7e7"
      },
      "comment": {
        "color": "#6e6a86"
      },
      "comment_preproc": {
        "color": "#c4a7e7"
      },
      "keyword": {
        "color": "#c4a7e7"
      },
      "keyword_reserved": {
        "color": "#c4a7e7"
      },
      "keyword_namespace": {
        "color": "#c4a7e7"
      },
      "keyword_type": {
        "color": "#9ccfd8"
      },
      "operator": {
        "color": "#c4a7e7"
      },
      "punctuation": {
        "color": "#e0def4"
      },
      "name": {
        "color": "#9ccfd8"
      },
      "name_builtin": {
        "color": "#9ccfd8"
      },
      "name_tag": {
        "color": "#c4a7e7"
      },
      "name_attribute": {
        "color": "#31748f"
      },
      "name_class": {
        "color": "#9ccfd8"
      },
      "name_constant": {
        "color": "#c4a7e7"
      },
      "name_decorator": {
        "color": "#31748f"
      },
      "name_function": {
        "color": "#31748f"
      },
      "literal_number": {
        "color": "#31748f"
      },
      "literal_string": {
        "color": "#f6c177"
      },
      "literal_string_escape": {
        "color": "#c4a7e7"
      },
      "generic_deleted": {
        "color": "#c4a7e7"
      },
      "generic_emph": {
        "color": "#f6c177",
        "italic": true
      },
      "generic_inserted": {
        "color": "#31748f"
      },
      "generic_strong": {
        "color": "#f6c177",
        "bold": true
      },
      "generic_subheading": {
        "color": "#c4a7e7"
      },
      "background": {
        "background_color": "#191724"
      }
    }
  }
}
