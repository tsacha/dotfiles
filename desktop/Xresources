!urxvt extensions:

URxvt.perl-ext-common:  default,selection-to-clipboard,matcher
URxvt.iso14755: False
URxvt.keysym.Shift-Control-V: eval:paste_clipboard

!enabling clickable links:

URxvt.url-select.launcher: /usr/bin/firefox
URxvt.matcher.button: 0
URxvt.url-select.underline: false
URxvt.keysym.C-Delete: perl:matcher:last
URxvt.keysym.M-Delete: perl:matcher:list

!urxvt scrolling options and cursor style:

URxvt*saveLines: 12000
